{
  "name": "phoenix-fe",
  "version": "1.0.0",
  "description": "Phoenix AI Personal Assistant - Frontend with Capacitor for iOS/macOS",
  "author": "Phoenix Team",
  "scripts": {
    "dev": "python3 -m http.server 8000",
    "cap:init": "npx cap init",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync ios",
    "cap:copy": "npx cap copy ios",
    "cap:open:ios": "npx cap open ios",
    "cap:build": "npx cap copy && npx cap sync",
    "watch:ios": "node scripts/watch-sync.js",
    "dev:ios": "npm run watch:ios",
    "build:ios": "npx cap sync ios && npx cap build ios",
    "test": "node test-full-flow.js"
  },
  "dependencies": {
    "@capacitor-community/capacitor-googlemaps-native": "^1.2.0",
    "@capacitor-community/contacts": "^5.1.1",
    "@capacitor-community/keep-awake": "^7.1.0",
    "@capacitor-community/privacy-screen": "^6.0.0",
    "@capacitor-community/screen-brightness": "^7.0.0",
    "@capacitor-community/speech-recognition": "^5.0.0",
    "@capacitor/action-sheet": "^5.0.7",
    "@capacitor/app-launcher": "^5.0.8",
    "@capacitor/browser": "^5.2.1",
    "@capacitor/camera": "^5.0.10",
    "@capacitor/clipboard": "^5.0.8",
    "@capacitor/core": "^5.7.8",
    "@capacitor/device": "^5.0.8",
    "@capacitor/dialog": "^5.0.8",
    "@capacitor/filesystem": "^5.2.2",
    "@capacitor/geolocation": "^5.0.8",
    "@capacitor/haptics": "^5.0.8",
    "@capacitor/ios": "^5.7.8",
    "@capacitor/keyboard": "^5.0.9",
    "@capacitor/local-notifications": "^5.0.8",
    "@capacitor/motion": "^5.0.8",
    "@capacitor/network": "^5.0.8",
    "@capacitor/push-notifications": "^5.1.2",
    "@capacitor/share": "^5.0.8",
    "@capacitor/status-bar": "^5.0.8",
    "@capacitor/toast": "^5.0.8",
    "@capawesome/capacitor-badge": "^7.0.1",
    "@capgo/native-audio": "^6.0.14",
    "@perfood/capacitor-healthkit": "^1.3.2",
    "@transistorsoft/capacitor-background-fetch": "^5.2.5",
    "@transistorsoft/capacitor-background-geolocation": "^5.4.2",
    "axios": "^1.13.1",
    "capacitor-apple-pay": "^0.2.1",
    "capacitor-email-composer": "^7.0.2",
    "capacitor-plugin-siri-shorts": "^5.0.0",
    "capacitor-video-player": "^6.0.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.7.8",
    "chokidar": "^5.0.0",
    "husky": "^9.1.7",
    "puppeteer": "^21.11.0",
    "typescript": "^5.9.3"
  },
  "capacitor": {
    "appId": "com.phoenix.ai",
    "appName": "Phoenix AI",
    "webDir": "www",
    "bundledWebRuntime": false
  }
}
