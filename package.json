{
  "name": "phoenix-fe",
  "version": "1.0.0",
  "description": "Phoenix AI Personal Assistant - Frontend with Capacitor for iOS/macOS",
  "author": "Phoenix Team",
  "scripts": {
    "dev": "python3 -m http.server 8000",
    "cap:init": "npx cap init",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:build": "npx cap copy && npx cap sync",
    "test": "node test-full-flow.js"
  },
  "dependencies": {
    "@aparajita/capacitor-biometric-auth": "^9.1.2",
    "@capacitor-community/speech-recognition": "^5.0.0",
    "@capacitor/camera": "^5.0.10",
    "@capacitor/core": "^5.5.1",
    "@capacitor/device": "^5.0.8",
    "@capacitor/filesystem": "^5.2.2",
    "@capacitor/haptics": "^5.0.8",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/local-notifications": "^5.0.8",
    "@capacitor/network": "^5.0.8",
    "@capacitor/push-notifications": "^5.1.2",
    "@capacitor/share": "^5.0.8",
    "axios": "^1.13.1",
    "capacitor-plugin-siri-shorts": "^5.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "puppeteer": "^21.11.0",
    "typescript": "^5.9.3"
  },
  "capacitor": {
    "appId": "com.phoenix.ai",
    "appName": "Phoenix AI",
    "webDir": ".",
    "bundledWebRuntime": false
  }
}
