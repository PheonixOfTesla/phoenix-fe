{
  "version": 2,
  "buildCommand": "",
  "installCommand": "npm install",
  "devCommand": "python3 -m http.server 8000",
  "outputDirectory": "www",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/onboarding",
      "destination": "/onboarding.html"
    },
    {
      "source": "/mercury",
      "destination": "/mercury.html"
    },
    {
      "source": "/venus",
      "destination": "/venus.html"
    },
    {
      "source": "/earth",
      "destination": "/earth.html"
    },
    {
      "source": "/mars",
      "destination": "/mars.html"
    },
    {
      "source": "/jupiter",
      "destination": "/jupiter.html"
    },
    {
      "source": "/saturn",
      "destination": "/saturn.html"
    },
    {
      "source": "/uranus",
      "destination": "/uranus.html"
    },
    {
      "source": "/neptune",
      "destination": "/neptune.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/src/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
